{
    "manifest_version": 2,
    "name": "ChatGPT Toolkit",
    "version": "0.1.0",
    "description": "Make chat.openai.com better with an opinionated set of extensions",
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "webRequest",
      "webNavigation",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  