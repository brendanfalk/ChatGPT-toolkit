{
    "manifest_version": 3,
    "name": "Use GPT-4 in ChatGPT",
    "version": "0.2.1",
    "description": "Automatically selects the GPT-4 model in new chats in ChatGPT",
    "icons": {
      "48": "icon.png"
    },
    "permissions": [
      "webNavigation",
      "tabs"
    ],
    "host_permissions" : [
      "https://chat.openai.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  